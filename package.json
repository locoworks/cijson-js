{
  "name": "cijson-js",
  "version": "1.0.0",
  "description": "CIJSON JS Lib",
  "author": "Rajiv Seelam",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "dotenv -- turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "features/*"
  ],
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "dotenv-cli": "^7.1.0"
  },
  "devDependencies": {
    "prettier": "^2.8.4",
    "turbo": "^1.8.3"
  }
}
